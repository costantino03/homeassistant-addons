{
    "name": "Keycloak",
    "version": "26.02.3",
    "slug": "keycloak",
    "description": "Host Keycloak on Home Assistant OS",
    "arch": [
        "amd64",
        "aarch64"
    ],
    "ingress":true,
    "ingress_port": 8080,
    "panel_title": "Keycloak",
    "panel_icon": "mdi:account-key",
    "startup": "services",
    "boot": "auto",
    "options": {
        "db_host": "localhost",
        "db_database": "keycloak",
        "db_username": "keycloak",
        "db_password": "keycloak",
